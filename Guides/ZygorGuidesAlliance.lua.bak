local function loadGuide(filename)
	-- This will look for a .lua file that returns the guide content as a string
	local success, content = pcall(dofile, "zygor_guides/alliance/" .. filename .. ".zygor_guide")

	if not success then
		error("Could not load guide file: " .. filename .. ".zygor_guide - " .. tostring(content))
	end
	return content
end

